{
  "name": "eruda-benchmark",
  "version": "2.0.0",
  "description": "Eruda plugin for running JavaScript benchmarks",
  "main": "eruda-benchmark.js",
  "scripts": {
    "dev": "webpack-dev-server --host 0.0.0.0",
    "build": "webpack && webpack -p",
    "ci": "npm run lint && npm run build",
    "lint": "eslint src/**/*.js",
    "format": "prettier src/index.js src/style.scss *.js index.html --write"
  },
  "keywords": [
    "eruda",
    "benchmark",
    "plugin"
  ],
  "author": "redhoodsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liriliri/eruda-benchmark/issues"
  },
  "homepage": "https://github.com/liriliri/eruda-benchmark#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "benchmark": "^2.1.4",
    "css-loader": "^0.28.7",
    "eruda": "^2.0.0",
    "eslint": "^6.8.0",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.6.0",
    "lodash": "^4.17.5",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.14",
    "postcss-class-prefix": "^0.3.0",
    "postcss-loader": "^2.0.9",
    "prettier": "^1.19.1",
    "sass-loader": "^6.0.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
